# 教程  https://www.jianshu.com/p/e3cfae830985  iOS用workspace和cocoapods管理多个项目
workspace 'RFDemo.xcworkspace'
#project 'RFDebugCategoryDemo/RFDebugCategoryDemo.xcodeproj'

target 'RFDebugCategoryDemo' do
  platform :ios, '9.0'
  project 'RFDebugCategoryDemo/RFDebugCategoryDemo.xcodeproj' 
  pod 'Masonry', '~> 1.0.2'
  pod 'Aspects'
end

target 'RFKVODemo' do
  platform :ios, '9.0'
  project 'RFKVODemo/RFKVODemo.xcodeproj' 
  pod 'Masonry', '~> 1.0.2'
  pod 'AFNetworking', '~> 3.1.0'
end


target 'RFKVODemo2' do
  platform :ios, '9.0'
  project 'RFKVODemo2/RFKVODemo2.xcodeproj' 
 

end

target 'RFRuntimeDemo' do
    platform :ios, '9.0'
    project 'RFRuntimeDemo/RFRuntimeDemo.xcodeproj'
    
    
end

target 'RFMVC_MVP_MVVM' do
    platform :ios, '9.0'
    project 'RFMVC_MVP_MVVM/RFMVC_MVP_MVVM.xcodeproj'
    pod 'Masonry', '~> 1.0.2'
    pod 'SVProgressHUD'
    
end

target 'FastCalculateBoundRectDemo' do
    platform :ios, '9.0'
    project 'FastCalculateBoundRectDemo/FastCalculateBoundRectDemo.xcodeproj'
    pod 'MLeaksFinder'
    
end








